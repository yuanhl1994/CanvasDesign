/*矩形*/
function rect(canvas,id) {
	var rect = new fabric.Rect({
		width: 100,
		height: 100,
		top:50,
		left:50,
		fill: '#fff',
		stroke:'black',
		opacity: 1,
		id:id
	});
	canvas.add(rect);
}
/**
圆形
*/
function circle(canvas,id) {
	var circle = new fabric.Circle({
		radius:100,
		top:50,
		left:50,
		fill: '#fff',
		stroke:'black',
		opacity: 1,
		id:id
	});
	canvas.add(circle);
}
