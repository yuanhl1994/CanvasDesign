/**
*编辑器主体布局样式
*/

body{height:100%; overflow:hidden; margin:0px; padding:0px;}
html{height:100%; overflow:hidden; margin:0px; padding:0px;}
*{
	margin: 0px;
	padding:0px;
	font-size: 14px;
	font-family:"Microsoft YaHei",微软雅黑;
}
li {
	list-style-type: none;
}
input[type="text"]{
	outline-color: rgb(245,96,76);
}
/*总div*/
.bod {
	width: 100%;
	height: 100%;
	border: none;
}
/*左边栏*/
.left {
	height:100%;
	width:200px;
	border:none;
	float:left;
	position: absolute;
}
/*左边栏上方菜单*/
.left_menu {
	width:100%;
	height:32px;
	border:none;
	border-bottom: 0.1px solid grey;
	top: 0px;
}
.left_tem:hover,
.left_sou:hover {
	cursor: pointer;
}
.left_tem,
.left_sou {
	width:100px;
	height:32px;
	border:none;
	/*border-radius: 5px;*/
	/*box-shadow: 0.1px 0.1px 0.1px 0.1px grey;*/
	float:left;
	text-align: center;
	vertical-align: middle;
	background-color: white;
	color:rgb(51,58,77);
	font-size: 15px;
}
.left_tem {
	background-color: rgb(240,240,240);
}
.left_tem span,
.left_sou span {
	position: absolute;
	margin-left: -10%;
    margin-top: 4%;
    font-size: 14px;
}

/*左边栏下部*/
.dis_tem {
	display: none;
}
.dis_sou {
	display:block;
}

.left_center {
	width:100%;
	height:95%;
	border:none;
}
.dis_tem,
.dis_sou {
	width:100%;
	height:100%;
	border:none;
	border-radius: 5px;
	/*box-shadow: 0.5px 0.5px 0.5px 0.5px grey; */
	text-align: center;
}
/*.dis_tem {
	overflow-y:scroll;

}*/
/*左边栏-素材库左边菜单*/
.sou_left {
	width:20%;
	height:100%;
	border-right: 0.5px solid black;
	float:left;	
}

.kindDiv {
	width:98%;
	height:10%;
	border:0.1px solid black;
	border-right: none;
	border-radius: 3px;
	background-color: rgb(241,250,254);
}
.kind_1,
.kind_2,
.kind_3,
.kind_4 {
	width:100%;
	height:100%;
}
.kindDiv:hover {
	background-color: rgb(152,214,246);
