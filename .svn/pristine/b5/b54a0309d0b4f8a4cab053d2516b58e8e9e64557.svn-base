}

/*左边栏 新*/
.template,
.background,
.decoration,
.frame,
.addPic {
	width:100%;
	height: 36px;
	border-top: none;
	border-bottom:1px solid #e0e0e0;
	/*border:0.1px solid #e0e0e0;*/
	font-size: 14px;
	color:#33394f;
}
.template {
	background-color: #f5604c;
	color:white;
}
.template:hover,
.background:hover,
.decoration:hover,
.frame:hover,
.addPic:hover {
	cursor: pointer;
}

.template_div,
.background_div,
.decoration_div,
.frame_div,
.addPic_div {
	width: 100%;
    height: 200px;
    border: none;
    display: none;
    /*overflow-y: auto;*/
    border-bottom: 1px solid #e0e0e0;
    background: #eaeaea;
}
.background_div{
	background: #eaeaea;
}
.template_div {
	display:block;
	background: #eaeaea;
}
.addPic_div {
	height: 400px;
	background: #eaeaea;
}
