/*左边栏-素材库主体*/
.sou_cen {
	width:80%;
	height:92%;
	left: 20%;
	position: absolute;
	float:right;
	border:none;
	box-shadow: 0.1px 0.1px 0.1px 0.1px grey;
}
.upload,
.sou_3 {
	width:100%;
	height:30%;
	border-bottom: 0.5px solid black;
	border-radius: 5px;
}

.upload {
	height:8%;
}
.upBtn {
	font-size: 30px;
	width:99%;
	height:100%;
	border:none;
	border-radius: 3px;
	box-shadow: 0.5px 0.5px 0.5px 0.5px grey;
	background-color: rgb(241,250,254);
}
.upBtn:hover {
	background-color: rgb(152,214,246);
	cursor: pointer;
}
.upInput {
	display:none;
}
.sou_3 {
	height:91.4%;
	border-bottom: none;
	overflow: auto;	
}
.upImg {
	width:45%;
	height:90px;
	border:none;
	box-shadow: 0.5px 0.5px 0.5px 0.5px grey;
	margin-left: 7px;
	margin-top: 5px;
	float:left;
}
.upImg img {
	width:100%;
	height:100%;
}
.sou_other {
	display:none;
	width:80%;
	height:92%;
	left: 20%;
	position: absolute;
	float:right;
	border:none;
	box-shadow: 0.1px 0.1px 0.1px 0.1px grey;
}

/*左边栏 模板库*/
.temDiv {
	width:86px;
	height:86px;
	/* border:0.1px solid black; */
	/*border:2px solid white;*/
	margin-top: 10px;
	margin-left: 9px;
	margin-right: 1px;
	float:left;
}
.temDiv:hover {
	cursor: pointer;
	border:2px solid #f5604c;
}
