	float:right;
	margin-right: 14px;
	margin-top: -26px;
}
.left div span {
	display: block;
    /*margin-top: 8px;*/
	text-align: center;
	font-size: 14px;
}
.addPic_btn {
	display: block;
	margin: 0 auto;
	margin-top: 3px;
}
.addPic_btn:hover {
	cursor: pointer;
} 
.leftBtn {
	line-height: 36px;
}

/*右边栏*/
